			<Description>TXT_KEY_GAME_OPTION_CAN_STACK_IN_CITY</Description>
			<Help>TXT_KEY_GAME_OPTION_CAN_STACK_IN_CITY_HELP</Help>
		</Row>
	</GameOptions>
	<Language_en_US>
		<Row Tag="TXT_KEY_GAME_OPTION_CAN_STACK_IN_CITY">
			<Text>RED DLL: Unlimited stacking in cities</Text>
		</Row>
		<Row Tag="TXT_KEY_GAME_OPTION_CAN_STACK_IN_CITY_HELP">
